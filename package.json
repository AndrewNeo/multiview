{
  "name": "multiview-chrome",
  "version": "0.0.0",
  "description": "Multiview is a browser addon that lets you embed any website into a split page view.",
  "author": "Kauko <kauko@biosynth.link>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:src build:zip",
    "build:src": "parcel build src/*.html src/background.js src/oauth_handler.js --no-source-maps",
    "build:zip": "cd dist/ && bestzip ../multiview.zip *"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "bestzip": "^2.1.5",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.2"
  }
}
